{
    "name": "vite-plugin-posthog",
    "version": "0.1.0",
    "scripts": {
        "build": "rollup -c"
    },
    "main": "dist/index.js",
    "module": "src/core/index.ts",
    "types": "src/core/index.d.ts",
    "typings": "src/core/index.d.ts",
    "exports": {
        ".": {
          "import": "./src/core/index.ts",
          "require": "./dist/index.js"
        },
        "./react": {
          "import": "./src/react/index.ts",
          "require": "./dist/react.js"
        }
    },
    "type": "module",
    "devDependencies": {
        "@rollup/pluginutils": "^5.1.0",
        "rollup": "^4.9.5",
        "rollup-plugin-typescript2": "^0.36.0"
    },
    "dependencies": {
        "posthog-js": "^1.9.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "vite": "^5.0.8"
    }
}
